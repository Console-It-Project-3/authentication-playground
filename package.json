  },
  "homepage": "https://github.com/Console-It-Project-3/authentication-playground#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "concurrently": "^5.1.0",
    "connect-mongo": "^3.2.0",
    "express": "^4.18.2",
    "express-session": "^1.17.0",
    "if-env": "^1.0.4",
    "mongoose": "^5.9.2",
    "morgan": "^1.9.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7"
  }
